{
  "name": "blackjack-backend",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start:watch": "nodemon",
    "start": "npm run start-http",
    "start-http": "node -r tsconfig-paths/register -r ts-node/register ./src/server.ts",
    "build": "tsc",
    "start-concurrent": "concurrently --kill-others \"npm run start-http\" \"npm run start-ws\""
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^4.0.1",
    "config": "^3.3.0",
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.5",
    "ws": "^7.2.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.3",
    "@types/node": "^13.9.1",
    "concurrently": "^5.1.0",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.1",
    "file-loader": "^6.0.0",
    "nodemon": "^2.0.2",
    "ts-loader": "^6.2.2",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.0",
    "typescript": "^3.8.3"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm start",
    "ext": "ts, js"
  }
}
